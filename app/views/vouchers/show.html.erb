<div class="container">
  <h1>Voucher</h1>
  <div class="well">
    <% if notice %>
      <div class="alert alert-success">
        <button type="button" class="close" data-dismiss="alert">&times;</button>
        <%= notice %>
      </div>
    <% end %>

    <%= link_to 'Edit', edit_voucher_path(@voucher), :class => 'btn' %>
    <%= link_to 'Back', vouchers_path, :class => 'btn' %>
  </div>
</div>
